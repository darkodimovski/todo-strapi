{
  "kind": "collectionType",
  "collectionName": "todos",
  "info": {
    "singularName": "todo",
    "pluralName": "todos",
    "displayName": "Todo",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "dueDate": {
      "type": "date"
    },
    "position": {
      "type": "enumeration",
      "enum": [
        "todo",
        "in-progress",
        "done"
      ]
    },
    "assignee": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "todos"
    },
    "project": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::project.project",
      "inversedBy": "todos"
    },
    "descriptionHistory": {
      "type": "text"
    }
  }
}
